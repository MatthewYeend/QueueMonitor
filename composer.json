{
    "name": "mattyeend/queue-monitor",
    "description": "A Laravel package to monitor queue logs.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Queue",
        "Monitor",
        "Monitoring",
        "Queue-Monitor",
        "Queue-Monitoring",
        "Package"
    ],
    "authors": [
        {
            "name": "Matt Yeend",
            "email": "mail@mattyeend.co.uk"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "MattYeend\\QueueMonitoring\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MattYeend\\QueueMonitoring\\QueueMonitoringServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
